         var fruits = [ "apple", "orange", "mango" ];
        		  window.alert(fruits[0]);
         
		 //by using new operator
	     function display(name,eid) {
			  this.name=name;
			  this.eid=eid;
		  window.alert(this.name+" "+this.eid);
		  }

		  var obj=new display("harish",001);

          //using constructor
          var arr = new Array( 10, 20, 30 );
        document.write("arr.constructor is:" + arr[0]+ " " +arr[1]+" "+arr[2]); 
         
	 </script>

